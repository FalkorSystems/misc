[header]
	name Robot
	version 0000
	manufacturer Falkor Systems
	part RobotBone
	serial 0000

[current]
	3.3vdd 123mA
	5vdd 500mA
	5sys 2000mA
	supplied 1234mA

# UART1 for PWM Input
[pin p9-26] # Rx
     in
     fast
     rx
     pull down
     pull enable
     mux uart1_rxd

# UART1 for PWM Output
[pin p9-24] # Tx
     out
     fast
     pull down
     pull enable
     mux uart1_txd

# UART2 for GPS
[pin p9-22] # Rx
     in
     fast
     rx
     pull down
     pull enable
     mux uart2_rxd

[pin p9-21] # Tx
     out
     fast
     pull down
     pull enable
     mux uart2_txd

# UART4 for XBee
[pin p9-11] # Rx
     in
     fast
     rx
     pull down
     pull enable
     mux uart4_rxd

[pin p9-13] # Tx
     out
     fast
     pull down
     pull enable
     mux uart4_txd

# UART5 for Second XBee
[pin p8-38] # Rx
     in
     fast
     rx
     pull down
     pull enable
     mux uart5_rxd

[pin p8-37] # Tx
     out
     fast
     pull down
     pull enable
     mux uart5_txd

# I2C for IMU and Barometer
[pin p9-19]
     bidir
     fast
     pull up
     pull enable
     mux i2c2_scl

[pin p9-20]
     bidir
     fast
     pull up
     pull enable
     mux i2c2_sda

#
# No muxing required for analog
#
